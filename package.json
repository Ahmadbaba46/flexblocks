{
  "name": "flexblocks",
  "version": "1.0.0",
  "description": "CSS layout library",
  "scripts": {
    "build": "node-sass src/ -o dist/dev/ --source-map dist/dev/ --output-style expanded && node-sass src/ -o dist/prod/ --output-style compressed",
    "watch": "node-sass --watch src/ -o dist/ --source-map dist --output-style expanded",
    "test": "npm run build && bash -c '[[ $(git status ./dist --porcelain 2>/dev/null) == \"\" ]]'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lord/flexblocks.git"
  },
  "author": "Robert Lord",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lord/flexblocks/issues"
  },
  "homepage": "https://github.com/lord/flexblocks",
  "devDependencies": {
    "node-sass": "^3.2.0",
    "autoprefixer-cli":"^1.0.0"
  }
}
